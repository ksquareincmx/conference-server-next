{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "picture": {
      "type": "string",
      "default": ""
    },
    "name": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true,
      "default": "user"
    },
    "profile": {
      "type": "string"
    },
    "slackId": {
      "type": "string"
    },
    "googleId": {
      "type": "string"
    }
  },
  "hidden": ["slackAccessToken", "googleIdToken"],
  "validations": [],
  "relations": {
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "property": "slackLogin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "property": "googleLogin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
